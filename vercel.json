{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://api.deontwikkelplaats.nl/api/:path*"
        },
        {
            "source": "/api/:path*/",
            "destination": "https://api.deontwikkelplaats.nl/api/:path*"
        },
        {
            "source": "/socket.io*",
            "destination": "https://api.deontwikkelplaats.nl/socket.io/:path*"
        },
        {
            "source": "/socket.io/*",
            "destination": "https://api.deontwikkelplaats.nl/socket.io/:path*"
        }
    ]
}